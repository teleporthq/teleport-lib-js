{
  "name": "@teleporthq/teleport-lib-js",
  "version": "0.0.18",
  "description": "Teleport Core Library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "tsc -w",
    "build": "npm run clean && tsc",
    "typedoc": "typedoc --options typedoc.json src",
    "clean": "rm -rf dist",
    "lint": "tslint -p ./",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "npm run test -- --coverage --no-cache",
    "precommit": "pretty-quick --staged && npm run lint",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/teleporthq/teleport-lib-js.git"
  },
  "author": "teleportHQ",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.1.3",
    "codecov": "^3.0.2",
    "husky": "^0.14.3",
    "jest": "^23.2.0",
    "pretty-quick": "^1.6.0",
    "ts-jest": "^22.4.6",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.13.0",
    "typedoc": "^0.11.1",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "prettier": "^1.13.7",
    "@types/jss": "^9.5.4",
    "@types/prettier": "^1.13.1",
    "is-url": "^1.2.4",
    "isomorphic-fetch": "^2.2.1",
    "jss": "^9.8.7",
    "jss-preset-default": "^4.5.0"
  }
}
